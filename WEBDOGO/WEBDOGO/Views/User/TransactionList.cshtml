@model IEnumerable<dynamic>

<h2>Danh sách giao dịch SEPAY</h2>

@if (Model == null || !Model.Any())
{
    <p>Không có giao dịch nào.</p>
}
else
{
    <table>
        <thead>
            <tr>
                <th>ID Giao dịch</th>
                <th>Số tiền</th>
                <th>Trạng thái</th>
                <th>Thời gian</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.transactionId</td>
                    <td>@item.amount</td>
                    <td>@item.status</td>
                    <td>@item.timestamp</td>
                </tr>
            }
        </tbody>
    </table>
}
